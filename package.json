  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.9"
  },
  "dependencies": {
    "gulp-wait": "0.0.2",
    "jquery": "^3.3.1"
  },
  "main": "gulpfile.js",
