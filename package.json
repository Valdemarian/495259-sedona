{
  "name": "sed",
  "version": "13.0.0",
  "private": true,
  "description": "Личный проект «Седона» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "7.2.x",
    "browser-sync": "2.23.x",
    "gulp": "3.9.x",
    "gulp-cheerio": "^0.6.3",
    "gulp-gh-pages": "git+https://github.com/nicothin/gulp-gh-pages.git",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "7.0.x",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "3.1.x",
    "gulp-svgmin": "^1.2.4",
    "gulp-svgstore": "^6.1.1",
    "stylelint": "8.4.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint 'source/sass/**/*.scss' --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp style",
    "deploy": "gulp deploy",
    "start": "gulp serve"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
